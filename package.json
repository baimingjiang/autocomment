{
	"name": "autoheadcomment",
	"displayName": "AutoHeadComment",
	"description": "insert comment on your file.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.insertautoheadcomment",
				"title": "InsertAutoHeadComment"
			}
		],
		"keybindings":[
			{
				"command": "extension.insertautoheadcomment",
				"key": "CTRL+I"
			}
		],
		"configuration": {
			"type": "object",
			"title": "AutoComment Configuration",
			"properties": {
				"autocomment.autor": {
					"type": "string",
					"default": "autocomment",
					"description": "文件创建者"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
